{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "paths": {
      "@/": ["index"],
      "@/config/*": ["config/*"],
      "@/seeds/*": ["seeds/*"],
      "@/models/*": ["models/*"],
      "@/scripts/*": ["scripts/*"],
      "@/utils/*": ["utils/*"],
      "@/middlewares/*": ["middlewares/*"],
      "@/routes/*": ["routes/*"],
    },
    // "skipLibCheck": true,
    "baseUrl": ".",
    "outDir": "./dist"
  }
}
